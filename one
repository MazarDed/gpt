    $(document).ready(function(){
      $('.topmenu ul li').hover(
          function() {
              $(this).addClass("active");
              $(this).find('ul').stop(true, true); // –æ—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º –≤—Å—é —Ç–µ–∫—É—â—É—é –∞–Ω–∏–º–∞—Ü–∏—é
              $(this).find('ul').slideDown();
          },
          function() {
              $(this).removeClass("active");        
              $(this).find('ul').slideUp('fast');
          }
      );
  });
    
    
  
    
    
    
    
    
