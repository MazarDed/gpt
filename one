<script>

function openModals() {




	vkBridge.send("VKWebAppAllowMessagesFromGroup", {"group_id": 221489796, "key": "cc964fabb0e1d7092"})
	

		.then(data => {
	
		console.log(data);
		if (data.result == true) {

// –æ—Ç–∫—Ä—ã–≤–∞–µ–º –æ–∫–Ω–æ –µ—Å–ª–∏ –ø–æ–¥–ø–∏—Å–∞–ª—Å—è. 
document.getElementById("myModal_sd").style.display = "block";

			
             
            
            
            
vkBridge.send('VKWebAppJoinGroup', {
  group_id: 221489796
  })
  .then((data) => { 
    if (data.result) {
      // –ü–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—å –ø–æ–¥–ø–∏—Å–∞–ª—Å—è –Ω–∞ —Å–æ–æ–±—â–µ—Å—Ç–≤–æ
    }
  })
  .catch((error) => {
    // –û—à–∏–±–∫–∞
    console.log(error);
  });           
            
            
            
            
            
            
            
	
	VK.Widgets.AllowMessagesFromCommunity("vk_allow_messages_from_community", {height: 30}, 221489796);
    
    VK.Observer.subscribe("widgets.allowMessagesFromCommunity.allowed", function f(userId) {
        console.log(userId);
        console.log("allowed");
        
        // –¥–µ–π—Å—Ç–≤–∏–µ –µ—Å–ª–∏ –æ—Ç–ø–∏—Å–∞–ª—Å—è
        
        
        
        
        
      });
      VK.Observer.subscribe("widgets.allowMessagesFromCommunity.denied", function f(userId) {
        console.log(userId);
        console.log("denied");
      });
    
    
	  }
	 
	  })
	  .catch(error => {
		// Handling an error
        dalRedir();
        
	  });








	
 
  
}
</script>







